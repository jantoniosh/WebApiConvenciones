{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "AllowedHosts": "*",
  "ConnectionStrings": {
    "DefaultConnection": "*****"
  },
  "Cors": {
    "AllowedOrigins": [
      "http://localhost:4200",
      "http://convenciones.mensajito.mx"
    ]
  },
  "Serilog": {
    "MinimumLevel": "Information",
    "WriteTo": [
      {
        "Name": "MSSqlServer",
        "Args": {
          "connectionString": "*****",
          "tableName": "Logs",
          "autoCreateSqlTable": true
        }
      }
    ]
  }
}
